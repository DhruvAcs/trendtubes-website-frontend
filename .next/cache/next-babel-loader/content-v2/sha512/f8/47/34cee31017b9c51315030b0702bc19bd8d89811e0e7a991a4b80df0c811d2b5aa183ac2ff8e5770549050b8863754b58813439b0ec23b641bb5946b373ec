rvices/' + id);\n  },\n  getCMSPath: function getCMSPath(path) {\n    console.log('getcmspath', path);\n    return axios.get(API_PREURL + '/cms/', {\n      params: {\n        path: path\n      }\n    });\n  },\n  editCMSPath: function editCMSPath(path, subPathData) {\n    return axios.post(API_PREURL + '/cms/?path=' + path, {\n      // params: { path: path },\n      subPaths: subPathData\n    });\n  },\n  getUsers: function getUsers() {\n    return axios.get(API_PREURL + '/users/');\n  },\n  getUser: function getUser(id) {\n    return axios.get(API_PREURL + '/users/' + id);\n  },\n  getAuthInfo: function getAuthInfo(cookie) {\n    return axios.get(API_PREURL + '/auth/info', {\n      headers: {\n        cookie: cookie\n      }\n    });\n  },\n  authLogout: function authLogout(cookie) {\n    // var cookie = ctx.req.headers.cookie || '';\n    // removeCookies(ctx, 'trendtubes.session');\n    return axios.get(API_PREURL + '/auth/logout', {\n      headers: {\n        cookie: cookie\n      }\n    });\n  },\n  isAuthenticated: function isAuthenticated() {\n    return true;\n  }\n};\nexport { BackendApi };","map":{"version":3,"sources":["/home/trevorb/Documents/VSCode Projects/trendtubes-new/trendtubes/utils/backend-api.ts"],"names":["axios","API_PREURL","BackendApi","getSellixProductCache","get","getAccounts","getAccount","id","createAccount","body","post","editAccount","deleteAccount","getServices","getService","createService","editService","deleteService","getCMSPath","path","console","log","params","editCMSPath","subPathData","subPaths","getUsers","getUser","getAuthInfo","cookie","headers","authLogout","isAuthenticated"],"mappings":"AAAA,OAAOA,KAAP,MAA0C,OAA1C;AAGA,IAAMC,UAAU,GAAG,uBAAnB,C,CACA;AACA;;AAEA,IAAMC,UAAU,GAAG;AAClBC,EAAAA,qBAAqB,EAAE,iCAAM;AAC5B,WAAOH,KAAK,CAACI,GAAN,CAAUH,UAAU,GAAG,wBAAvB,CAAP;AACA,GAHiB;AAIlBI,EAAAA,WAAW,EAAE,uBAAM;AAClB,WAAOL,KAAK,CAACI,GAAN,CAAUH,UAAU,GAAG,WAAvB,CAAP;AACA,GANiB;AAOlBK,EAAAA,UAAU,EAAE,oBAACC,EAAD,EAAQ;AACnB,WAAOP,KAAK,CAA