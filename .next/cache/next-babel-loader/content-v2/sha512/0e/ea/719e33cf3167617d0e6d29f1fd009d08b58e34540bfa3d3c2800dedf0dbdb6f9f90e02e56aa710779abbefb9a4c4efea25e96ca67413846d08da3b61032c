rvices/' + id);\n  },\n  getCMSPath: function getCMSPath(path) {\n    console.log('getcmspath', path);\n    return axios.get(API_PREURL + '/cms/', {\n      params: {\n        path: path\n      }\n    });\n  },\n  getUsers: function getUsers() {\n    return axios.get(API_PREURL + '/users/');\n  },\n  getAuthInfo: function getAuthInfo() {\n    return axios.get(API_PREURL + '/auth/info');\n  },\n  authLogout: function authLogout() {\n    return axios.get(API_PREURL + '/auth/logout');\n  }\n};\nexport { BackendApi };","map":{"version":3,"sources":["/home/trevorb/Documents/VSCode Projects/trendtubes/utils/backend-api.ts"],"names":["axios","API_PREURL","BackendApi","getShoppyProductCache","get","getAccounts","getAccount","id","createAccount","body","post","editAccount","deleteAccount","getServices","getService","createService","editService","deleteService","getCMSPath","path","console","log","params","getUsers","getAuthInfo","authLogout"],"mappings":"AAAA,OAAOA,KAAP,MAA0C,OAA1C;AAEA,IAAMC,UAAU,GAAG,uBAAnB,C,CACA;AACA;;AAEA,IAAMC,UAAU,GAAG;AAClBC,EAAAA,qBAAqB,EAAE,iCAAM;AAC5B,WAAOH,KAAK,CAACI,GAAN,CAAUH,UAAU,GAAG,wBAAvB,CAAP;AACA,GAHiB;AAIlBI,EAAAA,WAAW,EAAE,uBAAM;AAClB,WAAOL,KAAK,CAACI,GAAN,CAAUH,UAAU,GAAG,WAAvB,CAAP;AACA,GANiB;AAOlBK,EAAAA,UAAU,EAAE,oBAACC,EAAD,EAAQ;AACnB,WAAOP,KAAK,CAACI,GAAN,CAAUH,UAAU,GAAG,YAAb,GAA4BM,EAAtC,CAAP;AACA,GATiB;AAUlBC,EAAAA,aAAa,EAAE,uBAACC,IAAD,EAAU;AACxB,WAAOT,KAAK,CAACU,IAAN,CAAWT,UAAU,GAAG,WAAxB,EAAqCQ,IAArC,CAAP;AACA,GAZiB;AAalBE,EAAAA,WAAW,EAAE,qBAACJ,EAAD,EAAKE,IAAL,EAAc;AAC1B,WAAOT,KAAK,CAACU,IAAN,CAAWT,UAAU,GAAG,YAAb,GAA4BM,EAAvC,EAA2CE,IAA3C,CAAP;AACA,GAfiB;AAgBlBG,EAAAA,aAAa,EAAE,uBAACL,EAAD,EAAQ;AACtB,WAAOP,KAAK,UAAL,CAAaC,UAAU,GAAG,YAAb,GAA4BM,EAAzC,CAAP;AACA,GAlBiB;AAmBlBM,EAAAA,WAAW,EAAE,uBAAM;AAClB,WAAOb,KAAK,CAACI,GAAN,CAAUH,UAAU,GAAG,WAAvB,CAAP;AACA,GArBiB;AAsBlBa,EAAAA,UAAU,EAAE,oBAACP,EAAD,EAAQ;AACnB,WAAOP,KAAK,CAACI,GAAN,CAAUH,UAAU,GAAG,YAAb,GAA4BM,EAAtC,CAAP;AACA,GAxBiB;AAyBlBQ,EAAAA,aAAa,EAAE,uBAACN,IAAD,EAAU;AACxB,W