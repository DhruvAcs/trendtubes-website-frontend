n    return axios[\"delete\"](API_PREURL + '/services/' + id);\n  },\n  getCMSPath: function getCMSPath(path) {\n    console.log('getcmspath', path);\n    return axios.get(API_PREURL + '/cms/', {\n      params: {\n        path: path\n      }\n    });\n  },\n  getUsers: function getUsers() {\n    return axios.get(API_PREURL + '/users/');\n  },\n  getAuthInfo: function getAuthInfo(cookie) {\n    return axios.get(API_PREURL + '/auth/info', {\n      headers: {\n        cookie: cookie\n      }\n    });\n  },\n  authLogout: function authLogout(ctx) {\n    var cookie = ctx.req.headers.cookie || '';\n    removeCookies(ctx, 'trendtubes.session');\n    return axios.get(API_PREURL + '/auth/logout', {\n      headers: {\n        cookie: cookie\n      }\n    });\n  },\n  isAuthenticated: function isAuthenticated() {\n    return true;\n  }\n};\nexport { BackendApi };","map":{"version":3,"sources":["/home/trevorb/Documents/VSCode Projects/trendtubes/utils/backend-api.ts"],"names":["axios","removeCookies","API_PREURL","BackendApi","getShoppyProductCache","get","getAccounts","getAccount","id","createAccount","body","post","editAccount","deleteAccount","getServices","getService","createService","editService","deleteService","getCMSPath","path","console","log","params","getUsers","getAuthInfo","cookie","headers","authLogout","ctx","req","isAuthenticated"],"mappings":"AAAA,OAAOA,KAAP,MAA0C,OAA1C;AACA,SAASC,aAAT,QAA8B,cAA9B;AAEA,IAAMC,UAAU,GAAG,uBAAnB,C,CACA;AACA;;AAEA,IAAMC,UAAU,GAAG;AAClBC,EAAAA,qBAAqB,EAAE,iCAAM;AAC5B,WAAOJ,KAAK,CAACK,GAAN,CAAUH,UAAU,GAAG,wBAAvB,CAAP;AACA,GAHiB;AAIlBI,EAAAA,WAAW,EAAE,uBAAM;AAClB,WAAON,KAAK,CAACK,GAAN,CAAUH,UAAU,GAAG,WAAvB,CAAP;AACA,GANiB;AAOlBK,EAAAA,UAAU,EAAE,oBAACC,EAAD,EAAQ;AACnB,WAAOR,KAAK,CAACK,GAAN,CAAUH,UAAU,GAAG,YAAb,GAA4BM,EAAtC,CAAP;AACA,GATiB;AAUlBC,EAAAA,aAAa,EAAE,uBAACC,IAAD,EAAU;AACxB,WAAOV,KAAK,CAACW,IAAN,CAAWT,UAAU,GAAG,WAAxB,EAAqCQ,IAArC,CAAP;AACA,GAZiB;AAalBE,EAAAA,WAAW,EAAE,qBAACJ,EAAD,EAAKE,IAAL,EAAc;AAC1B,WAAOV,KAAK,CAACW,IAAN,CAA